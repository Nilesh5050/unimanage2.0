@page
@{
    ViewData["Title"] = "Contact Us - Unimanage Gym Software India";
    ViewData["Description"] = "Contact Unimanage Gym Software for support and inquiries. Get in touch with our team for gym management solutions in India via phone, email, or visit our Noida office.";
    ViewData["Keywords"] = "Contact Unimanage, Gym Software Support India, Gym Management Helpdesk, Fitness Software Contact";
    ViewData["Canonical"] = "https://unimanage.tech/contact";
    ViewData["OgImage"] = "https://unimanage.tech/images/contact-og.jpg";
    Layout = "_Layout";
}

<!-- Structured Data - Contact Page -->
<script type="application/ld+json">
    {
      "@@context": "https://schema.org",
      "@@type": "ContactPage",
      "mainEntity": {
        "@@type": "Organization",
        "name": "Unimanage Technologies",
        "url": "https://unimanage.tech/",
        "logo": "https://unimanage.tech/images/logo.png",
        "image": "https://unimanage.tech/images/contact-og.jpg",
        "description": "Best gym management software in India for fitness clubs and gyms",
        "address": {
          "@@type": "PostalAddress",
          "streetAddress": "M-3, Gali No. 7, Mamura, Sector 66",
          "addressLocality": "Noida",
          "addressRegion": "Uttar Pradesh",
          "postalCode": "201301",
          "addressCountry": "IN"
        },
        "contactPoint": {
          "@@type": "ContactPoint",
          "telephone": "+919205807124",
          "contactType": "customer service",
          "email": "support@unimanage.tech",
          "availableLanguage": ["English", "Hindi"],
          "contactOption": ["TollFree", "HearingImpairedSupported"]
        },
        "openingHours": [
          {
            "@@type": "OpeningHoursSpecification",
            "dayOfWeek": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday"],
            "opens": "09:00",
            "closes": "18:00"
          },
          {
            "@@type": "OpeningHoursSpecification",
            "dayOfWeek": ["Saturday"],
            "opens": "10:00",
            "closes": "16:00"
          }
        ],
        "sameAs": [
          "https://www.facebook.com/share/1BV25U342s/",
          "https://x.com/unimanage?t=WsxmJYOTXxyNUqmaN1_4eg&s=09",
          "https://www.instagram.com/unimanage/?igsh=enB6ZmVpdDR5NTd0#",
          "https://www.linkedin.com/company/unimanage-technologies"

        ]
      }
    }
</script>

<!-- Structured Data - FAQ -->
<script type="application/ld+json">
    {
      "@@context": "https://schema.org",
      "@@type": "FAQPage",
      "mainEntity": [
        {
          "@@type": "Question",
          "name": "What is the best way to contact Unimanage support?",
          "acceptedAnswer": {
            "@@type": "Answer",
            "text": "The fastest way to reach our support team is by calling +91 9205807124 during business hours. You can also email us at support@unimanage.tech or fill out the contact form on this page."
          }
        },
        {
          "@@type": "Question",
          "name": "Do you provide on-site support for gyms in India?",
          "acceptedAnswer": {
            "@@type": "Answer",
            "text": "Yes, we provide on-site support for gyms in major cities across India. Please contact us to schedule an on-site visit from our technical team."
          }
        },
        {
          "@@type": "Question",
          "name": "What are your business hours?",
          "acceptedAnswer": {
            "@@type": "Answer",
            "text": "Our business hours are Monday to Friday from 9am to 6pm, and Saturday from 10am to 4pm. We're closed on Sundays, but our online support system is available 24/7."
          }
        },
        {
          "@@type": "Question",
          "name": "How quickly can I expect a response to my inquiry?",
          "acceptedAnswer": {
            "@@type": "Answer",
            "text": "We typically respond to all inquiries within 24 business hours. For urgent technical issues, please call our support hotline for immediate assistance."
          }
        },
        {
          "@@type": "Question",
          "name": "Do you offer training for your gym management software?",
          "acceptedAnswer": {
            "@@type": "Answer",
            "text": "Yes, we offer comprehensive training sessions for all our gym management software packages. Training can be conducted online or on-site at your facility anywhere in India."
          }
        }
      ]
    }
</script>

<!-- Breadcrumb Navigation -->
<nav aria-label="breadcrumb" class="bg-gray-100 py-2">
    <div class="container mx-auto px-4 sm:px-6">
        <ol class="breadcrumb flex items-center text-sm" itemscope itemtype="https://schema.org/BreadcrumbList">
            <li class="breadcrumb-item" itemprop="itemListElement" itemscope itemtype="https://schema.org/ListItem">
                <a href="/" itemprop="item"><span itemprop="name">Home</span></a>
                <meta itemprop="position" content="1" />
            </li>
            <li class="breadcrumb-item active" itemprop="itemListElement" itemscope itemtype="https://schema.org/ListItem">
                <span itemprop="name">Contact Us</span>
                <meta itemprop="position" content="2" />
            </li>
        </ol>
    </div>
</nav>

