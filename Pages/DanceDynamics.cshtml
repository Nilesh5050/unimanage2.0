@page
@{
    ViewData["Title"] = "Dance Dynamics";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dance Dynamics - Premier Dance Studio</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <link rel="stylesheet" href="~/css/dancedynamics.css" />
    
</head>
<body>
    <!-- Navigation -->
    <nav id="navbar" class="fixed w-full z-50 py-4 px-4 md:px-6">
        <div class="container mx-auto flex justify-between items-center">
            <div class="flex items-center space-x-2">
                <i class="fas fa-music text-2xl md:text-3xl text-indigo-500"></i>
                <span class="text-xl md:text-2xl font-bold gradient-text">Dance Dynamics</span>
            </div>

            <!-- Desktop Menu -->
            <div class="hidden md:flex space-x-8">
                <a href="#home" class="nav-link hover:text-indigo-500 transition">Home</a>
                <a href="#about" class="nav-link hover:text-indigo-500 transition">About</a>
                <a href="#classes" class="nav-link hover:text-indigo-500 transition">Classes</a>
                <a href="#schedule" class="nav-link hover:text-indigo-500 transition">Schedule</a>
                <a href="#instructors" class="nav-link hover:text-indigo-500 transition">Instructors</a>
                <a href="#contact" class="nav-link hover:text-indigo-500 transition">Contact</a>
            </div>

            <!-- Mobile Menu Button -->
            <button id="mobile-menu-btn" class="md:hidden text-2xl">
                <i class="fas fa-bars"></i>
            </button>
        </div>
    </nav>

    <!-- Mobile Menu -->
    <div id="mobile-menu" class="mobile-menu fixed top-0 right-0 h-full w-64 bg-white shadow-lg z-50 p-6">
        <button id="close-menu" class="absolute top-4 right-4 text-2xl">
            <i class="fas fa-times"></i>
        </button>
        <div class="mt-12 flex flex-col space-y-6">
            <a href="#home" class="nav-link hover:text-indigo-500 transition text-lg">Home</a>
            <a href="#about" class="nav-link hover:text-indigo-500 transition text-lg">About</a>
            <a href="#classes" class="nav-link hover:text-indigo-500 transition text-lg">Classes</a>
            <a href="#schedule" class="nav-link hover:text-indigo-500 transition text-lg">Schedule</a>
            <a href="#instructors" class="nav-link hover:text-indigo-500 transition text-lg">Instructors</a>
            <a href="#contact" class="nav-link hover:text-indigo-500 transition text-lg">Contact</a>
        </div>
    </div>

    <!-- Hero Section -->
    <section id="home" class="hero-bg min-h-screen flex items-center justify-center text-white relative">
        <div class="absolute inset-0 bg-black opacity-30"></div>
        <div class="container mx-auto px-4 md:px-6 text-center relative z-10">
            <h1 class="heading-font text-4xl md:text-7xl font-bold mb-4 md:mb-6 animate-fadeInUp">Dance Dynamics</h1>
            <p class="text-lg md:text-2xl mb-6 md:mb-8 animate-fadeInUp" style="animation-delay: 0.2s">Where rhythm meets passion</p>
            <div class="flex flex-col sm:flex-row justify-center gap-4 animate-fadeInUp" style="animation-delay: 0.4s">
                <a href="#classes" class="btn-primary bg-indigo-600 hover:bg-indigo-700 text-white font-bold py-3 px-6 md:px-8 rounded-full transition transform hover:scale-105 text-sm md:text-base">
                    Explore Classes
                </a>
                <a href="#schedule" class="btn-primary bg-pink-500 hover:bg-pink-600 text-white font-bold py-3 px-6 md:px-8 rounded-full transition transform hover:scale-105 text-sm md:text-base">
                    View Schedule
                </a>
            </div>
            <div class="mt-12 md:mt-16 animate-float">
                <i class="fas fa-chevron-down text-2xl md:text-3xl"></i>
            </div>
        </div>
    </section>

    <!-- About Section -->
    <section id="about" class="py-12 md:py-20 bg-gray-50">
        <div class="container mx-auto px-4 md:px-6">
            <div class="text-center mb-12 md:mb-16 scroll-reveal">
                <h2 class="text-3xl md:text-4xl font-bold mb-4">About Dance Dynamics</h2>
                <div class="w-24 h-1 bg-indigo-500 mx-auto"></div>
            </div>

            <div class="grid md:grid-cols-2 gap-8 md:gap-12 items-center">
                <div class="scroll-reveal">
                    <img src="~/images/dance.png">
                </div>
                <div class="scroll-reveal">
                    <h3 class="text-2xl md:text-3xl font-bold mb-4">Your Journey to Dance Excellence</h3>
                    <p class="text-gray-600 mb-4">
                        Founded in 2010, Dance Dynamics has been the premier destination for dance enthusiasts of all ages and skill levels. Our state-of-the-art facility and expert instructors create the perfect environment for you to explore, learn, and excel in various dance styles.
                    </p>
                    <p class="text-gray-600 mb-6">
                        Whether you're a beginner taking your first steps or an experienced dancer looking to refine your technique, we offer a comprehensive range of classes tailored to meet your needs and aspirations.
                    </p>
                    <div class="grid grid-cols-2 gap-4">
                        <div class="bg-white p-4 rounded-lg shadow">
                            <i class="fas fa-users text-2xl md:text-3xl text-indigo-500 mb-2"></i>
                            <h4 class="font-bold text-sm md:text-base">500+ Students</h4>
                        </div>
                        <div class="bg-white p-4 rounded-lg shadow">
                            <i class="fas fa-calendar-check text-2xl md:text-3xl text-pink-500 mb-2"></i>
                            <h4 class="font-bold text-sm md:text-base">50+ Weekly Classes</h4>
                        </div>
                        <div class="bg-white p-4 rounded-lg shadow">
                            <i class="fas fa-award text-2xl md:text-3xl text-indigo-500 mb-2"></i>
                            <h4 class="font-bold text-sm md:text-base">Award Winning</h4>
                        </div>
                        <div class="bg-white p-4 rounded-lg shadow">
                            <i class="fas fa-globe text-2xl md:text-3xl text-pink-500 mb-2"></i>
                            <h4 class="font-bold text-sm md:text-base">10 Dance Styles</h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Classes Section -->
    <section id="classes" class="py-12 md:py-20">
        <div class="container mx-auto px-4 md:px-6">
            <div class="text-center mb-12 md:mb-16 scroll-reveal">
                <h2 class="text-3xl md:text-4xl font-bold mb-4">Our Dance Classes</h2>
                <div class="w-24 h-1 bg-indigo-500 mx-auto"></div>
                <p class="mt-4 text-gray-600 max-w-2xl mx-auto">Discover your passion with our diverse range of dance styles, taught by industry professionals</p>
            </div>

            <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 md:gap-8">
                <!-- Ballet -->
                <div class="dance-card bg-white rounded-xl shadow-lg overflow-hidden scroll-reveal">
                    <div class="h-40 md:h-48 bg-gradient-to-r from-purple-500 to-indigo-600 flex items-center justify-center">
                        <i class="fas fa-user-tie text-white text-5xl md:text-6xl"></i>
                    </div>
                    <div class="p-4 md:p-6">
                        <h3 class="text-lg md:text-xl font-bold mb-2">Ballet</h3>
                        <p class="text-gray-600 mb-4 text-sm md:text-base">Classical ballet techniques for grace, strength, and precision. Perfect for all ages and skill levels.</p>
                        <div class="flex justify-between items-center">
                            <span class="text-indigo-600 font-bold text-sm md:text-base">Beginner to Advanced</span>
                            <a href="#schedule" class="text-indigo-600 hover:text-indigo-800">
                                <i class="fas fa-arrow-right"></i>
                            </a>
                        </div>
                    </div>
                </div>

                <!-- Hip Hop -->
                <div class="dance-card bg-white rounded-xl shadow-lg overflow-hidden scroll-reveal">
                    <div class="h-40 md:h-48 bg-gradient-to-r from-yellow-500 to-orange-500 flex items-center justify-center">
                        <i class="fas fa-compact-disc text-white text-5xl md:text-6xl"></i>
                    </div>
                    <div class="p-4 md:p-6">
                        <h3 class="text-lg md:text-xl font-bold mb-2">Hip Hop</h3>
                        <p class="text-gray-600 mb-4 text-sm md:text-base">Urban dance styles with high-energy moves and freestyle expression to contemporary beats.</p>
                        <div class="flex justify-between items-center">
                            <span class="text-indigo-600 font-bold text-sm md:text-base">All Levels</span>
                            <a href="#schedule" class="text-indigo-600 hover:text-indigo-800">
                                <i class="fas fa-arrow-right"></i>
                            </a>
                        </div>
                    </div>
                </div>

                <!-- Contemporary -->
                <div class="dance-card bg-white rounded-xl shadow-lg overflow-hidden scroll-reveal">
                    <div class="h-40 md:h-48 bg-gradient-to-r from-blue-500 to-teal-500 flex items-center justify-center">
                        <i class="fas fa-water text-white text-5xl md:text-6xl"></i>
                    </div>
                    <div class="p-4 md:p-6">
                        <h3 class="text-lg md:text-xl font-bold mb-2">Contemporary</h3>
                        <p class="text-gray-600 mb-4 text-sm md:text-base">Fluid movements combining elements of modern, jazz, and ballet for expressive storytelling.</p>
                        <div class="flex justify-between items-center">
                            <span class="text-indigo-600 font-bold text-sm md:text-base">Intermediate+</span>
                            <a href="#schedule" class="text-indigo-600 hover:text-indigo-800">
                                <i class="fas fa-arrow-right"></i>
                            </a>
                        </div>
                    </div>
                </div>

                <!-- Salsa -->
                <div class="dance-card bg-white rounded-xl shadow-lg overflow-hidden scroll-reveal">
                    <div class="h-40 md:h-48 bg-gradient-to-r from-red-500 to-pink-500 flex items-center justify-center">
                        <i class="fas fa-fire text-white text-5xl md:text-6xl"></i>
                    </div>
                    <div class="p-4 md:p-6">
                        <h3 class="text-lg md:text-xl font-bold mb-2">Salsa</h3>
                        <p class="text-gray-600 mb-4 text-sm md:text-base">Passionate Latin dance with vibrant rhythms and intricate partner work.</p>
                        <div class="flex justify-between items-center">
                            <span class="text-indigo-600 font-bold text-sm md:text-base">Beginner to Advanced</span>
                            <a href="#schedule" class="text-indigo-600 hover:text-indigo-800">
                                <i class="fas fa-arrow-right"></i>
                            </a>
                        </div>
                    </div>
                </div>

                <!-- Jazz -->
                <div class="dance-card bg-white rounded-xl shadow-lg overflow-hidden scroll-reveal">
                    <div class="h-40 md:h-48 bg-gradient-to-r from-green-500 to-emerald-500 flex items-center justify-center">
                        <i class="fas fa-music text-white text-5xl md:text-6xl"></i>
                    </div>
                    <div class="p-4 md:p-6">
                        <h3 class="text-lg md:text-xl font-bold mb-2">Jazz</h3>
                        <p class="text-gray-600 mb-4 text-sm md:text-base">Energetic and stylized dance form with sharp movements and theatrical expression.</p>
                        <div class="flex justify-between items-center">
                            <span class="text-indigo-600 font-bold text-sm md:text-base">All Levels</span>
                            <a href="#schedule" class="text-indigo-600 hover:text-indigo-800">
                                <i class="fas fa-arrow-right"></i>
                            </a>
                        </div>
                    </div>
                </div>

                <!-- Tap -->
                <div class="dance-card bg-white rounded-xl shadow-lg overflow-hidden scroll-reveal">
                    <div class="h-40 md:h-48 bg-gradient-to-r from-gray-700 to-gray-900 flex items-center justify-center">
                        <i class="fas fa-shoe-prints text-white text-5xl md:text-6xl"></i>
                    </div>
                    <div class="p-4 md:p-6">
                        <h3 class="text-lg md:text-xl font-bold mb-2">Tap</h3>
                        <p class="text-gray-600 mb-4 text-sm md:text-base">Rhythmic footwork creating percussive sounds. A unique blend of dance and music.</p>
                        <div class="flex justify-between items-center">
                            <span class="text-indigo-600 font-bold text-sm md:text-base">All Levels</span>
                            <a href="#schedule" class="text-indigo-600 hover:text-indigo-800">
                                <i class="fas fa-arrow-right"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Schedule Section -->
    <section id="schedule" class="py-12 md:py-20 bg-gray-50">
        <div class="container mx-auto px-4 md:px-6">
            <div class="text-center mb-12 md:mb-16 scroll-reveal">
                <h2 class="text-3xl md:text-4xl font-bold mb-4">Class Schedule</h2>
                <div class="w-24 h-1 bg-indigo-500 mx-auto"></div>
                <p class="mt-4 text-gray-600 max-w-2xl mx-auto">Find the perfect class time that fits your schedule</p>
            </div>

            <div class="bg-white rounded-xl shadow-lg p-4 md:p-6 scroll-reveal">
                <div class="flex flex-wrap gap-2 md:gap-4 mb-6 justify-center schedule-filter-container">
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-indigo-600 text-white rounded-full text-sm md:text-base" data-day="all">All Days</button>
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-gray-200 text-gray-700 rounded-full hover:bg-gray-300 text-sm md:text-base" data-day="monday">Monday</button>
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-gray-200 text-gray-700 rounded-full hover:bg-gray-300 text-sm md:text-base" data-day="tuesday">Tuesday</button>
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-gray-200 text-gray-700 rounded-full hover:bg-gray-300 text-sm md:text-base" data-day="wednesday">Wednesday</button>
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-gray-200 text-gray-700 rounded-full hover:bg-gray-300 text-sm md:text-base" data-day="thursday">Thursday</button>
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-gray-200 text-gray-700 rounded-full hover:bg-gray-300 text-sm md:text-base" data-day="friday">Friday</button>
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-gray-200 text-gray-700 rounded-full hover:bg-gray-300 text-sm md:text-base" data-day="saturday">Saturday</button>
                    <button class="schedule-filter px-3 py-2 md:px-4 md:py-2 bg-gray-200 text-gray-700 rounded-full hover:bg-gray-300 text-sm md:text-base" data-day="sunday">Sunday</button>
                </div>

                <div class="schedule-table">
                    <table class="w-full">
                        <thead>
                            <tr>
                                <th>Day</th>
                                <th>Time</th>
                                <th>Class</th>
                                <th>Instructor</th>
                                <th>Level</th>
                                <th>Studio</th>
                            </tr>
                        </thead>
                        <tbody id="schedule-body">
                            <tr data-day="monday">
                                <td>Monday</td>
                                <td>9:00 AM - 10:30 AM</td>
                                <td>Ballet Basics</td>
                                <td>Sarah Johnson</td>
                                <td>Beginner</td>
                                <td>Studio A</td>
                            </tr>
                            <tr data-day="monday">
                                <td>Monday</td>
                                <td>6:00 PM - 7:30 PM</td>
                                <td>Hip Hop Fusion</td>
